{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "asantibanez/laravel-eloquent-state-machines": "^6.0",
        "aymanalhattami/filament-slim-scrollbar": "^2.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "bezhansalleh/filament-shield": "^3.2",
        "brick/money": "^0.10.1",
        "calebporzio/sushi": "^2.5",
        "codeat3/blade-file-icons": "^1.10",
        "codeat3/blade-simple-icons": "^7.16",
        "dyrynda/laravel-cascade-soft-deletes": "^4.5",
        "filament/filament": "^3.3.33",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "hugomyb/filament-error-mailer": "v3.2.0.3",
        "joaopaulolndev/filament-pdf-viewer": "^1.0",
        "lara-zeus/qr": "^1.1",
        "laravel/framework": "^11.9",
        "laravel/socialite": "^5.16",
        "laravel/telescope": "^5.15",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "moneyphp/money": "^4.7",
        "njxqlus/filament-relation-manager-component": "^1.0",
        "opcodesio/log-viewer": "^3.15",
        "orangehill/iseed": "^3.0",
        "rap2hpoutre/fast-excel": "^5.5",
        "revolution/laravel-google-sheets": "^7.1",
        "shanerbaner82/panel-roles": "*",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-pdf": "^1.5",
        "stechstudio/filament-impersonate": "^3.15",
        "tapp/filament-auditing": "^3.0",
        "vormkracht10/filament-mails": "^2.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-shift/blueprint": "^2.10",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
         "files": [
            "app/Helpers/helpers_money.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
